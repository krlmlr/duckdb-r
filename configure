#!/bin/sh

set -ex
exit 0

cd $(dirname $0)/src

if [ -f "${DUCKDB_R_PREBUILT_ARCHIVE}" ] && tar -xf ${DUCKDB_R_PREBUILT_ARCHIVE}; then
  find . -name "*.o" | xargs touch
  find . -name "*.o" | xargs ls -l
  ls -lR
  cp include/from-zip.mk Makevars.duckdb
else
  cp include/to-zip.mk Makevars.duckdb
fi
